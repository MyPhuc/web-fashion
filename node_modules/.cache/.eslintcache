om/search/photos?query=${tk}&page=${page}&per_page=${per_page}&client_id=jqudJd4kyErYliqHsrkG50m6CxXOiIIgft51KKQyuqs`;\n        fetch(url)\n        .then(function(res){\n            if (!res.ok) throw Error(res.statusText);\n            return res.json();\n        })\n        .then(function(data){\n            let pictures = data.results;\n            console.log(pictures);\n            let listPic=[];\n            pictures.forEach(function(p, index){\n                console.log(p.urls.regular);\n                listPic.push(<img src={p.urls.regular} height=\"150\"/>)\n            })\n            ReactDOM.render(listPic, document.getElementById(\"kqseachPic\"));\n        })\n        .catch(function(error){\n            console.log('Có lỗi nè :\\n', error);\n        });\n    }\n    render() {\n        const kq=\n        <div>\n            <form className=\"col-9 m-auto\">\n                <input ref={this.tukhoa} className=\"form-control\"/>\n                <button onClick={this.timHinh} type=\"button\" className=\"btn btn-dark\">Tìm hình</button>\n            </form>\n            <div id=\"kqSeachPic\"></div>\n        </div>\n        return (kq)\n    }\n}\n","/Users/lethimyphuc/Documents/ReactJs/namustore/src/index.js",[],["127","128"],"/Users/lethimyphuc/Documents/ReactJs/namustore/src/reportWebVitals.js",[],"/Users/lethimyphuc/Documents/ReactJs/namustore/src/App.js",[],"/Users/lethimyphuc/Documents/ReactJs/namustore/src/context/cart/CartState.js",[],"/Users/lethimyphuc/Documents/ReactJs/namustore/src/pages/Login.js",["129"],"/Users/lethimyphuc/Documents/ReactJs/namustore/src/pages/Products.js",[],"/Users/lethimyphuc/Documents/ReactJs/namustore/src/pages/CatbooksDetail.js",["130"],"import React, { useEffect, useState } from 'react'\r\nimport Item from '../components/item/Item';\r\nconst CatbooksDetail = ({ match }) => {\r\n    const [cat, setCat] = useState({});\r\n    const [listBooks, setListBooks] = useState([]);\r\n    const url = \"http://localhost:3000/\"\r